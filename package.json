{
  "name": "shipmozo-sdk",
  "version": "1.1.0",
  "description": "Node.js SDK for the Shipmozo API, supporting standard orders and MPS.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\"",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json && node -e \"require('fs').writeFileSync('dist/esm/package.json', JSON.stringify({type:'module'}))\"",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ananta-devs/shipmozo-sdk.git"
  },
  "keywords": [
    "shipmozo",
    "shipping",
    "api",
    "sdk",
    "logistics"
  ],
  "author": "Ananta Hazra",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "axios-retry": "^4.5.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^18.0.0",
    "axios-mock-adapter": "^2.1.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.0.0"
  }
}
